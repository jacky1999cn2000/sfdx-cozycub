<aura:component description="Game Dashboard Container" implements="flexipage:availableForAllPageTypes" access="global" controller="GameDashboardController">
>
        <aura:attribute name="games" type="String"/>
    
        <aura:attribute name="originalList" type="List"/>
        <aura:attribute name="filteredList" type="List"/>
    
        <aura:handler name="init" value="{!this}" action="{!c.init}"/>

        <div class="slds-grid">
            <div class="slds-col slds-size_7-of-12">

                <c:SearchBar/>

                <div class="slds-grid">
                    <div class="slds-col slds-size_1-of-12">
                        <c:ArrowLeft/>
                    </div>
                    <div class="slds-col slds-size_10-of-12">
                        <c:GameItemPanel gameList="{!v.filteredList}"/>
                    </div>
                    <div class="slds-col slds-size_1-of-12">
                        <c:ArrowRight/>
                    </div>
                </div>
                

            </div>
            
        </div>

</aura:component>	
