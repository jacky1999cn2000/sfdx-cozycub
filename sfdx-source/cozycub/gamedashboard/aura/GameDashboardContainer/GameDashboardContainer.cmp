<aura:component description="Game Dashboard Container" implements="flexipage:availableForAllPageTypes" access="global" controller="GameDashboardController">
>
        <aura:attribute name="startIndex" type="Integer" default="0"/>
        <aura:attribute name="endIndex" type="Integer"/>
    
        <aura:attribute name="originalList" type="List"/>
        <aura:attribute name="filteredList" type="List"/>
    
        <aura:handler name="init" value="{!this}" action="{!c.init}"/>

        <div class="slds-grid">
            <div class="slds-col slds-size_7-of-12">

                <c:SearchBar/>
                {!v.startIndex} ---- {!v.endIndex}
                <div class="slds-grid">
                    <div class="slds-col slds-size_1-of-12">
                        <c:ArrowLeft startIndex="{!v.startIndex}" endIndex="{!v.endIndex}" />
                    </div>
                    <div class="slds-col slds-size_10-of-12">
                        <c:GameItemPanel gameList="{!v.filteredList}" startIndex="{!v.startIndex}" endIndex="{!v.endIndex}" />
                    </div>
                    <div class="slds-col slds-size_1-of-12">
                        <c:ArrowRight startIndex="{!v.startIndex}" endIndex="{!v.endIndex}" totalRecords="{!v.filteredList.length}" />
                    </div>
                </div>
                

            </div>
            
        </div>

</aura:component>	
