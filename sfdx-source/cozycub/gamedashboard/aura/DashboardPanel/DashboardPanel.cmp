<aura:component>

    <div class="slds-grid slds-grid_vertical">

            <!-- header -->
            <div class="slds-col"> 
                <div class="rounds">
                    <div class="slds-grid slds-grid_align-center">
                        <div class="slds-col"> 
                            <c:RoundNumber round="1"/>
                        </div>
                        <div class="slds-col"> 
                            <c:RoundNumber round="2"/>
                        </div>
                        <div class="slds-col"> 
                            <c:RoundNumber round="3"/>
                        </div>
                        <div class="slds-col"> 
                            <c:RoundNumber round="4"/>
                        </div>
                        <div class="slds-col"> 
                            <c:RoundNumber round="5"/>
                        </div>
                        <div class="slds-col"> 
                            <c:RoundNumber round="6"/>
                        </div>
                    </div>
                </div>
            </div>
    
            <!-- content -->
            <div class="slds-col">
                <div class="card-container">
                    <div class="card">
                        <div class="side">
                            front
                        </div>
                        <div class="side back">
                            back
                        </div>
                    </div>
                </div>
            </div>

            <!-- bottom -->
            <div class="slds-col">
                bottom 
            </div>
    
        </div>

    

</aura:component>	
